										 data-group-cd="{{:groupCd}}" data-menu-type="{{:displayCategoryCodeMain}}">
										<img src="{{:itemFileName != null ? itemFileName : itemBaseImage}}?{{:imgCache}}" alt="{{:itemName}}" onError="this.src='/images/common/noneImage.jpg'">
